    <script>

        $(document).on('click', '#videoPreview', function (e) {

            const videoSource = $(this).attr('data-src');

            $('#TVFAM source').attr('src', videoSource);
            $('#TVFAM')[0].load();
            $('#TVFAM')[0].play();

        });


    </script>

}

