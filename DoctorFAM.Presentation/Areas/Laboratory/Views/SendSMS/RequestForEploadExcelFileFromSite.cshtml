@model DoctorFAM.Domain.ViewModels.Admin.FamilyDoctor.RequestForUploadExcelFileFromDoctorsToSiteViewModel

@{
    ViewData["Title"] = @Localizer["آپلود فایل اکسل خود"];
}

@section Breadcrumb
    {
    <li>
        <a href="javascript:void(0)"> @ViewData["Title"] </a>
    </li>
}

<div class="row">
    <div class="col-lg-12 col-sm-12">

        <div class="widget">
            <div class="widget-header bordered-bottom bordered-green">
                <span class="widget-caption"> @Localizer["لطفا فایل خود را انتخاب کنید"] </span>
            </div>
            <div class="widget-body">
                <div>

                    <form asp-action="RequestForEploadExcelFileFromSite" asp-controller="SendSMS" asp-area="Laboratory" method="post" enctype="multipart/form-data">
                        <div class="modal-body">
                            <div class="row">

                                <div class="form-group col-md-12 col-sm-12">
                                    <input type="file" class="form-control" asp-for="ExcelFile">
                                </div>

                                <div class="clearfix"></div>

                                <div class="form-group col-md-12">
                                    <button type="submit" class="btn btn-success">@Localizer["ثبت"]</button>
                                </div>

                            </div>
                        </div>

                    </form>

                </div>
            </div>
        </div>
    </div>
</div>


