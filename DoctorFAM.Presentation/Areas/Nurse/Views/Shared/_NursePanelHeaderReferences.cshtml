@{
    var currentCulture = ApplicationCultures.GetCurrentCulture();
}


<link rel="shortcut icon" href="~/Content/images/Logo/doctorfamTes.png" type="image/x-icon">
<!--Basic Styles-->
<link href="/admin/css/bootstrap.min.css" rel="stylesheet" />
@if (currentCulture.IsRtl)
{
    <link href="/admin/css/bootstrap-rtl.min.css" rel="stylesheet" />
}
<link href="/admin/css/font-awesome.min.css" rel="stylesheet" />
<link href="/admin/css/weather-icons.min.css" rel="stylesheet" />

<!--Fonts-->
<style>
    @@font-face {
        font-family: 'WYekan';
        src: url('/admin/fonts/BYekan.woff') format('woff');
        font-weight: normal;
        font-style: normal;
    }
</style>
<link href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,600,700,300" rel="stylesheet" type="text/css">

<!--Beyond styles-->
@if (currentCulture.IsRtl)
{
    <link href="/admin/css/beyond-rtl.min.css" rel="stylesheet" />
    <link href="/admin/css/4095-rtl.min.css" rel="stylesheet" type="text/css" />
}
else
{
    <link href="/admin/css/beyond.min.css" rel="stylesheet" />
    <link href="/admin/css/4095.min.css" rel="stylesheet" type="text/css" />
}

<link href="/admin/css/demo.min.css" rel="stylesheet" />
<link href="/admin/css/typicons.min.css" rel="stylesheet" />
<link href="/admin/css/animate.min.css" rel="stylesheet" />

<!--Add By Developers-->
<link href="/common/notification/notifications.css" rel="stylesheet" />
<link href="/admin/css/AdminCustom.css" rel="stylesheet" />
<link href="/common/Custom/custom.css" rel="stylesheet" />
<link href="/common/Custom/NotificationBar.css" rel="stylesheet" />
<link id="skin-link" href="" rel="stylesheet" type="text/css" />

@*Manifest*@
<link rel="manifest" href="~/Site/Manifest/manifest.json">

<!--For Safari-->

<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black">
<meta name="apple-mobile-web-app-title" content="Doctor FAM">
<link rel="apple-touch-startup-image" href="href=" ~ /Site/Manifest/doctorfamLogo.png"">
<link rel="apple-touch-icon" sizes="57x57" href="~/Site/Manifest/doctorfamLogo.png">
<link rel="apple-touch-icon" sizes="76x76" href="~/Site/Manifest/doctorfamLogo.png">
<link rel="apple-touch-icon" sizes="114x114" href="~/Site/Manifest/doctorfamLogo.png">
<link rel="apple-touch-icon" sizes="167x167" href="~/Site/Manifest/doctorfamLogo.png">
<link rel="apple-touch-icon" sizes="144x144" href="~/Site/Manifest/doctorfamLogo.png">
<link rel="apple-touch-icon" sizes="152x152" href="~/Site/Manifest/doctorfamLogo.png">
<link rel="apple-touch-icon" sizes="180x180" href="~/Site/Manifest/doctorfamLogo.png">
<link rel="apple-touch-icon" sizes="192x192" href="~/Site/Manifest/doctorfamLogo.png">

<!--End For Safari-->
<!--IE-->

<meta name="msapplication-TileImage" content="~/Site/Manifest/doctorfamLogo.png">
<meta name="msapplication-TileColor" content="#3372DF">
<meta name="msapplication-starturl" content="/">

<!--End IE-->
<!-- Theme color for mobile users -->
<meta name="theme-color" content="#3f51b5">
